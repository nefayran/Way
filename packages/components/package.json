{
  "name": "@way/components",
  "version": "1.0.0",
  "description": "Milky way components system",
  "keywords": [
    "vue",
    "component",
    "typescript"
  ],
  "url": "https://github.com/nefayran/Way",
  "homepage": "https://github.com/nefayran/Way",
  "author": "https://github.com/nefayran",
  "license": "MIT",
  "main": "./dist/way-components.umd.js",
  "module": "./dist/way-components.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/way-components.es.js",
      "require": "./dist/way-components.umd.js"
    }
  },
  "scripts": {
    "start": "yarn test && vite",
    "lint": "eslint --ext .js,.ts,.vue",
    "build": "vite build && postcss dist/styles.css > dist/styles.min.css",
    "types": "vue-tsc --emitDeclarationOnly",
    "test": "jest --updateSnapshot --coverage"
  },
  "dependencies": {
    "@way/eslint-config": "^1.0.0",
    "@way/styles": "^1.0.0",
    "@way/tsconfig": "^1.0.0",
    "@way/fonts": "^1.0.0",
    "vue": "^3.2.16",
    "vuex": "^4.0.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.18",
    "cssnano": "^5.0.15",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "typescript": "4.5.4",
    "vite": "^2.7.12",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.30.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
